 require('fs');\r\nvar imgGen = require('js-image-generator');\r\n\r\n// Generate one image\r\nimgGen.generateImage(800, 600, 80, function(err, image) {\r\n    fs.writeFileSync('dummy.jpg', image.data);\r\n});\r\n\r\n// Generate multiple images\r\nfor(var i=1;i<=10;i++){\r\n    imgGen.generateImage(800, 600, 80, function(err, image) {\r\n        console.log(\"Generating image #\" +i);\r\n        fs.writeFileSync('dummy-' + i + '.jpg', image.data);\r\n    });\r\n}\r\n```\r\n","maintainers":[{"name":"ahupp","email":"ahupp@ashnet.de"}],"time":{"modified":"2023-03-07T15:37:56.084Z","created":"2016-05-06T11:43:15.516Z","1.0.0":"2016-05-06T11:43:15.516Z","1.0.1":"2016-05-06T12:10:43.664Z","1.0.2":"2016-05-08T10:28:18.637Z","1.0.3":"2020-09-04T10:42:37.292Z","1.0.4":"2023-03-07T15:37:55.989Z"},"homepage":"https://github.com/ashupp/js-image-generator#readme","keywords":["image","images","random","generate","generator","dummy","testdata","js","javascript","node"],"repository":{"type":"git","url":"git+https://github.com/ashupp/js-image-generator.git"},"author":{"name":"Alexander S. Hupp"},"bugs":{"url":"https://github.com/ashupp/js-image-generator/issues"},"license":"ISC","readmeFilename":"README.md","users":{"emilton":true}}