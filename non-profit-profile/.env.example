# API Configuration
REACT_APP_API_BASE_URL=http://localhost:3001/api

# Environment
REACT_APP_ENV=development

# Feature Flags
REACT_APP_ENABLE_AUTO_SAVE=true
REACT_APP_AUTO_SAVE_INTERVAL=30000

# File Upload Configuration
REACT_APP_MAX_FILE_SIZE=10485760  # 10MB in bytes
REACT_APP_ALLOWED_FILE_TYPES=.pdf,.doc,.docx,.jpg,.jpeg,.png

# Authentication
REACT_APP_AUTH_PROVIDER=local  # Options: local, oauth, saml

# Logging
REACT_APP_LOG_LEVEL=info  # Options: debug, info, warn, error

# Analytics (optional)
REACT_APP_ANALYTICS_ID=

# Error Tracking (optional)
REACT_APP_SENTRY_DSN=

# Export Configuration
REACT_APP_ENABLE_PDF_EXPORT=true
REACT_APP_ENABLE_CSV_EXPORT=true
REACT_APP_ENABLE_JSON_EXPORT=true